{
  "name": "@mexkyc/mcp",
  "version": "1.0.0",
  "description": "MexKYC - AI-powered KYC document extraction for Mexican business documents. Extract structured data from Acta Constitutiva, SAT Constancia, RFC, INE, FM2, bank statements, and utility bills. MCP server with profile building, validation, and compliance reporting.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mexkyc": "./dist/mcp/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mexkyc/mexkyc-mcp.git"
  },
  "homepage": "https://mexkyc.com",
  "bugs": {
    "url": "https://github.com/mexkyc/mexkyc-mcp/issues"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "check:model": "tsx src/index.ts",
    "test": "npm run test:kyc-pfds && npm run test:kyc-report-pfds && npm run test:mcp-pfds",
    "test:gemini": "tsx src/tests/testKycEndToEndGemini.ts",
    "test:constancia": "tsx src/tests/testConstancia.ts",
    "test:cfe": "tsx src/tests/testCfeProofOfAddress.ts",
    "test:bank": "tsx src/tests/testBankStatements.ts",
    "test:acta": "tsx src/tests/testActaCompanyIdentity.ts",
    "test:kyc-builder": "tsx src/tests/testKycProfileBuilder.ts",
    "test:kyc-pfds": "tsx src/tests/testKycEndToEndPFDS.ts",
    "test:kyc-report-pfds": "tsx src/tests/testKycReportPFDS.ts",
    "test:mcp-pfds": "tsx src/tests/testMcpToolsPFDS.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "client:list": "tsx src/scripts/clientManagement.ts list",
    "client:delete": "tsx src/scripts/clientManagement.ts delete",
    "client:open": "tsx src/scripts/clientManagement.ts open",
    "client:open-latest": "tsx src/scripts/clientManagement.ts open-latest",
    "client:run": "tsx src/scripts/runKycClient.ts",
    "client:reports": "tsx src/scripts/generateReports.ts",
    "archive:client": "tsx scripts/archive_client_runs.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "banana": "tsx scripts/nanoBananaGenerator.ts",
    "banana:all": "tsx scripts/nanoBananaGenerator.ts --all",
    "banana:list": "tsx scripts/nanoBananaGenerator.ts --list"
  },
  "keywords": [
    "mexkyc",
    "kyc",
    "kyc-mexico",
    "know-your-customer",
    "mexico-kyc",
    "document-extraction",
    "mcp",
    "model-context-protocol",
    "ai-document-processing",
    "typescript",
    "mexican-documents",
    "compliance",
    "aml",
    "anti-money-laundering",
    "financial-services",
    "fintech-mexico",
    "document-verification",
    "identity-verification",
    "ai-extraction",
    "structured-data",
    "validation",
    "profile-building",
    "acta-constitutiva",
    "sat-constancia",
    "rfc-validation",
    "ine-verification",
    "fm2",
    "bank-statements",
    "cfe-bill",
    "telmex-bill",
    "proof-of-address"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@google-cloud/aiplatform": "^5.13.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@prisma/client": "^6.19.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "fastify": "^5.6.2",
    "openai": "^4.77.0",
    "pdf-img-convert": "^2.0.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.394",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.19.0",
    "prom-client": "^15.1.3",
    "sharp": "^0.34.5",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
